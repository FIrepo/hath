        <rdfs:seeAlso rdf:datatype="&xsd;string">Object class definition and ObjectCharacteristics class definition</rdfs:seeAlso>
        <rdfs:range rdf:resource="http://multimedialab.elis.ugent.be/users/samcoppe/ontologies/Premis/premis.owl#ObjectCharacteristics"/>
        <rdfs:domain>
