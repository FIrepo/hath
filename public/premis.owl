        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://multimedialab.elis.ugent.be/users/samcoppe/ontologies/Premis/premis.owl#Bitstream"/>
                    <rdf:Description rdf:about="http://multimedialab.elis.ugent.be/users/samcoppe/ontologies/Premis/premis.owl#File"/>
