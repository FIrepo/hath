    <!-- http://multimedialab.elis.ugent.be/users/samcoppe/ontologies/Premis/premis.owl#preservationLevel -->

    <owl:ObjectProperty rdf:about="http://multimedialab.elis.ugent.be/users/samcoppe/ontologies/Premis/premis.owl#preservationLevel">
        <rdfs:seeAlso rdf:datatype="&xsd;string">Object class definition and PreservationLevel class definition</rdfs:seeAlso>
        <rdfs:range rdf:resource="http://multimedialab.elis.ugent.be/users/samcoppe/ontologies/Premis/premis.owl#PreservationLevel"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://multimedialab.elis.ugent.be/users/samcoppe/ontologies/Premis/premis.owl#File"/>
                    <rdf:Description rdf:about="http://multimedialab.elis.ugent.be/users/samcoppe/ontologies/Premis/premis.owl#Representation"/>
                </owl:unionOf>
