    <!-- http://multimedialab.elis.ugent.be/users/samcoppe/ontologies/Premis/premis.owl#signature -->

    <owl:ObjectProperty rdf:about="http://multimedialab.elis.ugent.be/users/samcoppe/ontologies/Premis/premis.owl#signature">
        <rdfs:seeAlso rdf:datatype="&xsd;string">SignatureInformation class definition and Signature class definition</rdfs:seeAlso>
        <rdfs:range rdf:resource="http://multimedialab.elis.ugent.be/users/samcoppe/ontologies/Premis/premis.owl#Signature"/>
